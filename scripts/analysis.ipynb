import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns

# Load dataset
df = pd.read_csv("../dataset/sales.csv")

# Display first rows
print(df.head())

# Summary statistics
print(df.describe())

# Plot sales trend
sns.lineplot(data=df, x="Month", y="Sales")
plt.title("Sales Trend")
plt.show()
